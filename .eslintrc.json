{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console": ["warn", { "allow": ["warn", "error"] }],
        "indent": ["error", "tab"],
        "no-tabs": "off",
        "space-before-function-paren": "off",
        "comma-dangle": "off",
        "no-plusplus": "off",
        "max-len": "off",
        "radix": "off",
        "no-unused-vars": "warn",
        "no-empty": "off",
        "object-curly-newline": "off",
        "class-methods-use-this": "off"
    }
}
